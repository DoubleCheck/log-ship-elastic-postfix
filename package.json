{
  "name": "log-ship-elasticsearch-postfix",
  "description": "Ship normalized Postfix logs to Elasticsearch",
  "version": "0.0.1",
  "private": false,
  "homepage": "https://github.com/DoubleCheck/log-ship-elasticsearch-postfix",
  "bugs": {
    "url": "https://github.com/DoubleCheck/log-ship-elasticsearch-postfix/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/DoubleCheck/log-ship-elasticsearch-postfix.git"
  },
  "dependencies": {
    "async": "^1.3.0",
    "elasticsearch": "*",
    "safe-log-reader": "*",
    "postfix-parser": "*",
    "ini": "^1.3.4"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "coveralls": "^2.11.2",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2"
  },
  "config": {
    "blanket": {
      "pattern": "lib"
    }
  },
  "scripts": {
    "start": "node index.js",
    "coveralls": "COVERAGE=1 ./node_modules/.bin/mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js",
    "test": "mocha"
  }
}
