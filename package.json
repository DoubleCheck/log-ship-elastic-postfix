{
  "name": "log-ship-elastic-postfix",
  "description": "Ship normalized Postfix logs to Elasticsearch",
  "version": "0.9.5",
  "private": false,
  "keywords": [
    "log", "postfix", "elasticsearch", "log ship"
  ],
  "homepage": "https://github.com/DoubleCheck/log-ship-elastic-postfix",
  "bugs": {
    "url": "https://github.com/DoubleCheck/log-ship-elastic-postfix/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/DoubleCheck/log-ship-elastic-postfix.git"
  },
  "dependencies": {
    "elasticsearch": "*",
    "ini": "^1.3.4",
    "moment-timezone": "*",
    "postfix-parser": ">=1.0.4",
    "safe-log-reader": ">=0.9.9"
  },
  "devDependencies": {
    "grunt": "*",
    "grunt-eslint": "*",
    "grunt-mocha-test": "*",
    "grunt-version-check": "*",
    "mocha": "*"
  },
  "config": {
    "blanket": {
      "pattern": [
        "server.js",
        "lib"
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    },
    "travis-cov": {
      "threshold": 80
    }
  },
  "license" : "BSD-2-Clause",
  "scripts": {
    "start": "node server.js",
    "test": "NODE_ENV=test mocha"
  }
}
